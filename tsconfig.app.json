/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "allowJs": true,
    "types": ["node"]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "src/translations/translations.ts",
    "src/translations/translations.es.ts",
    "src/translations/translations.en.ts",
    "src/translations/dynamic-translations.en.ts",
    "src/translations/dynamic-translations.es.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
